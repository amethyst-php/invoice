{
    "name": "amethyst/invoice",
    "keywords": ["laravel", "amethyst", "amethyst-invoice", "invoice"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "amethyst/common": "0.1.*",
        "amethyst/legal-entity": "0.1.*",
        "amethyst/taxonomy": "0.1.*",
        "amethyst/file-generator": "0.1.*",
        "amethyst/work": "0.1.*",
        "amethyst/listener": "0.1.*",
        "amethyst/tax": "0.1.*",
        "league/iso3166": "^2.1",
        "mossadal/math-parser": "^1.3",
        "moneyphp/money": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\Database\\Seeds\\": "database/seeds/",
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\InvoiceServiceProvider"
            ]
        }
    }
}
